<!DOCTYPE html [
    <!ELEMENT body (script|iframe|canvas)*>
    <!ELEMENT head (link|style|script)*>

    <!ELEMENT link EMPTY>
    <!ELEMENT script (#PCDATA)>
    <!ELEMENT style (#PCDATA)>
    <!ELEMENT iframe EMPTY>
    <!ELEMENT canvas EMPTY>

    <!ATTLIST html xmlns CDATA #FIXED "http://www.w3.org/1999/xhtml">
    <!ATTLIST head xmlns CDATA #FIXED "http://www.w3.org/1999/xhtml">
    <!ATTLIST body xmlns CDATA #FIXED "http://www.w3.org/1999/xhtml">
    <!ATTLIST script
        xmlns CDATA #FIXED "http://www.w3.org/1999/xhtml"
        src CDATA #IMPLIED>
    <!ATTLIST style
        xmlns CDATA #FIXED "http://www.w3.org/1999/xhtml">
    <!ATTLIST link
        xmlns CDATA #FIXED "http://www.w3.org/1999/xhtml"
        rel CDATA #IMPLIED
        href CDATA #IMPLIED>
    <!ATTLIST iframe
        xmlns CDATA #FIXED "http://www.w3.org/1999/xhtml"
        is CDATA #FIXED "foo-bar"
        onerror CDATA #FIXED "alert(1)"
        onload CDATA #FIXED "alert(1)"
        src CDATA #FIXED "javascript:alert(1)"
        srcdoc CDATA #FIXED "foo">
    <!ATTLIST canvas
        xmlns CDATA #FIXED "http://www.w3.org/1999/xhtml">
]>
<html>
    <head>
        <link rel="import" href="./bower_components/polymer/polymer.html"/>
        <script src="./bower_components/webcomponentsjs/webcomponents.js"></script>
        <script src="debug.js"></script>
    </head>
    <body>
        <script>
          window.run = function() {
            var FooBar = Polymer({
              is: 'foo-bar',

              extends: 'iframe',

              created: function() {
                console.log("busted", this);
                debugger;
              }
            });

            var el1 = document.createElement('iframe');
            console.log("el1 is iframe", el1 instanceof HTMLIFrameElement);

            var el2 = document.createElement('iframe', 'foo-bar');
            console.log("el2 is iframe", el2 instanceof HTMLIFrameElement);
          };
        </script>
    </body>
</html>
